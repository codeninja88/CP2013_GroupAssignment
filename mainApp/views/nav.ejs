<div id="container">

    <div id="dropDownMenu" value="show" onclick="expandMenu()" style="border: 1px solid black;">
        <p></p>
        <nav>

            <div id="menu">

                <ul>

                    <% navMenu.forEach(function(nav) { %>
                    <li><a href="<%= nav.navButton %>" name="<%= nav.buttonName %>" <% if (title === nav.buttonName) {%>style="<%= currentPage %>"> <%} else {%>><%}%> <%= nav.buttonName %></a></li>
                    <% }); %>
                </ul>

            </div>
        </nav>
    </div>

</div>

<p id='msg'><%= msg %></p>

<p id='user'>
	<%= user %>
	<% if (isLoggedIn === true){ %>
		<a href='/logout'>LOG OUT</a>
	<%}%>
</p>